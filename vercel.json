{
  "buildCommand": "bundle install --path vendor/bundle && bundle exec jekyll build",
  "outputDirectory": "_site",
  "framework": "jekyll",
  "installCommand": "gem install bundler --user-install",
  "env": {
    "BUNDLE_GEMFILE": "Gemfile",
    "BUNDLE_PATH": "vendor/bundle",
    "BUNDLE_USER_HOME": "~/.bundle"
  }
}
